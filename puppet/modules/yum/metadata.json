{
  "name": "example42-yum",
  "version": "2.1.18",
  "author": "Alessandro Franceschi",
  "summary": "Puppet module for yum",
  "license": "Apache-2.0",
  "source": "https://github.com/example42/puppet-yum",
  "project_page": "http://www.example42.com",
  "issues_url": "https://github.com/example42/puppet-yum/issues",
  "description": "This module installs and manages yum and several yum repostories",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
